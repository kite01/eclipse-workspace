package chapter03;

public class OperatorEx15 {

	public static void main(String[] args) {
		int num = 0B00000101;	// 5
		
		System.out.println(num << 2);
		System.out.println(num >> 2);
		System.out.println(num >>> 2);
		
		System.out.println(num);
		
		num = num << 2;
		System.out.println(num);
	}

}
